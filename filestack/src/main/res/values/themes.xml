<resources>
  <style name="Theme.Filestackandroid" parent="Theme.AppCompat.Light"/>
    <style name="Theme.Filestackandroid.Fullscreen" parent="Theme.Filestackandroid">
        <item name="android:actionBarStyle">
            @style/Widget.Theme.Filestackandroid.ActionBar.Fullscreen
        </item>
        <item name="android:windowActionBarOverlay">true</item>
        <item name="android:windowBackground">@null</item>
